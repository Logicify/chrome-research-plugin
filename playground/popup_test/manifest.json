{
  "name": "Sample Extension Commands extension",
  "description": "Press Ctrl+Shift+Y to open the window",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "debugger"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y"
      }
    }
  }
}
